    "license": "MIT",
    "config": {
        "site": "showcase"
    },
    "scripts": {
        "android": "tns run android --bundle",
        "ios": "tns run ios --bundle",
        "ng": "ng",
        "start": "ng serve",
        "build": "./node_modules/@angular/cli/bin/ng build --configuration=production",
        "build-themes": "bash ./tools/build-themes.sh",
        "test": "./node_modules/@angular/cli/bin/ng test --watch=false",
        "test-cc": "./node_modules/@angular/cli/bin/ng test --watch=false --code-coverage",
        "lint": "./node_modules/@angular/cli/bin/ng lint",
        "e2e": "./node_modules/@angular/cli/bin/ng e2e",
        "build:ssr": "npm run build:client-and-server && npm run build:prerender",
        "build:client-and-server": "ng build --configuration=production --project=$npm_package_config_site && ng run $npm_package_config_site:server:production",
        "webpack:ssr": "webpack --config webpack.server.config.js --progress --colors",
        "serve:ssr": "sed -i '/module.exports = require(\".\")/d' ./functions/dist/server/main.js && node functions/dist/server.js",
        "server:ssr": "npm run webpack:ssr && npm run serve:ssr",
        "build:prerender": "npm run webpack:ssr && npm run generate:prerender",
        "build:prerender:firebase": "npm run compile:server && npm run generate:prerender",
        "build:firebase:ssr": "npm run build:client-and-server:store && npm run compile:server",
        "compile:server": "tsc -p server.tsconfig.json",
        "generate:prerender": "node ./functions/dist/prerender.js",
        "serve:prerender": "cd functions/dist/browser && http-server",
        "firebase:ssr": "npm run build:firebase:ssr",
        "serve:firebase:ssr": "sed -i '/module.exports = require(\".\")/d' ./functions/dist/server/main.js && node functions/dist/server"
    },
