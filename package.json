    "scripts": {
        "android": "tns run android --bundle",
        "ios": "tns run ios --bundle",
        "ng": "ng",
        "start": "ng serve",
        "build": "./node_modules/@angular/cli/bin/ng build --configuration=production --project=store",
        "build-themes": "bash ./tools/build-themes.sh",
        "test": "./node_modules/@angular/cli/bin/ng test --watch=false --project=store",
        "test-cc": "./node_modules/@angular/cli/bin/ng test --watch=false --project=store --code-coverage",
        "lint": "./node_modules/@angular/cli/bin/ng lint --project=store",
        "e2e": "./node_modules/@angular/cli/bin/ng e2e",
        "build-universal": "npm run build:ssr",
        "build-universal2": "npm run build:ssr2",
        "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:ssr",
        "serve:ssr": "sed -i '/module.exports = require(\".\")/d' ./functions/dist/server/main.js && node functions/dist/server.js",
        "build:client-and-server-bundles": "ng build --configuration=production --project=store && ng run store:server:production",
        "webpack:ssr": "webpack --config webpack.server.config.js --progress --colors",
        "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
        "build:ssr2": "npm run build:client-and-server-bundles && npm run compile:server",
        "compile:server": "tsc -p server.tsconfig.json",
        "generate:prerender": "cd dist && node prerender",
        "serve:prerender": "cd functions/dist/browser && http-server",
        "serve:ssr2": "sed -i '/module.exports = require(\".\")/d' ./functions/dist/server/main.js && node functions/dist/server"
    },
