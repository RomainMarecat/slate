{
  "extends": "../tsconfig.spec.json",
  "compilerOptions": {
    "paths": {
      "shared/*": [
        "../shared/*"
      ]
    },
    "outDir": "../out-tsc/spec",
    "baseUrl": "./",
    "module": "commonjs",
    "target": "es5",
    "types": [
      "jasmine",
      "node"
    ]
  },
  "files": [
    "test.ts"
  ],
  "include": [
    "**/*.spec.ts",
    "**/*.d.ts",
    "../shared/*.spec.ts",
    "../shared/**/*.spec.ts"
  ]
}
