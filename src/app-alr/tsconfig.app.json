{
  "extends": "../tsconfig.app.json",
  "compilerOptions": {
    "paths": {
      "shared/*": [
        "../shared/*"
      ]
    },
    "outDir": "../../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": []
  },
  "include": [
    "main.ts",
    "polyfills.ts",
    "../shared/*",
    "../shared/**/*.ts"
  ],
  "exclude": [
    "test.ts",
    "../shared/i18n/translate-server-loader.service.ts",
    "../shared/i18n/translates-server/*.ts",
    "**/*.spec.ts",
    "../shared/*.spec.ts",
    "../shared/**/*.spec.ts",
    "**/*.tns.ts",
    "**/*.android.ts",
    "**/*.ios.ts"
  ]
}
